# OpenAI
OPENAI_MODEL=gpt-5-nano
OPENAI_API_KEY=YOUR_OPENAI_API_KEY

# Core
JOBAGENT_OUTPUT_DIR=output
JOBAGENT_SCORE_KEEP_THRESHOLD=70
JOBAGENT_USE_LLM_SCORING=true
JOBAGENT_APPLY_BLOCKER_CAP=true

# Database (example patterns)
JOBAGENT_DATABASE_URL=mssql+pyodbc:///?odbc_connect=DRIVER%3D%7BODBC+Driver+18+for+SQL+Server%7D%3BSERVER%3D<DB_HOST>%2C<DB_PORT>%3BDATABASE%3D<DB_NAME>%3BUID%3D<DB_USER>%3BPWD%3D<DB_PASSWORD>%3BEncrypt%3Dyes%3BTrustServerCertificate%3Dno%3BConnection+Timeout%3D30
JOBAGENT_DATABASE_MIGRATOR_URL=mssql+pyodbc:///?odbc_connect=DRIVER%3D%7BODBC+Driver+18+for+SQL+Server%7D%3BSERVER%3D<DB_HOST>%2C<DB_PORT>%3BDATABASE%3D<DB_NAME>%3BUID%3D<DB_MIGRATOR_USER>%3BPWD%3D<DB_MIGRATOR_PASSWORD>%3BEncrypt%3Dyes%3BTrustServerCertificate%3Dno%3BConnection+Timeout%3D30
JOBAGENT_DB_ECHO=false
JOBAGENT_DB_POOL_SIZE=5
JOBAGENT_DB_MAX_OVERFLOW=10

# StepStone seeds
JOBAGENT_STEPSTONE_SEEDS_FILE=config/stepstone_seeds.json
# JOBAGENT_STEPSTONE_SEEDS_JSON=...

# Fetch / Playwright
JOBAGENT_USE_PLAYWRIGHT=true
JOBAGENT_HEADLESS=true
JOBAGENT_FETCH_USER_AGENT=Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/122.0.0.0 Safari/537.36
JOBAGENT_FETCH_ACCEPT_LANGUAGE=de-DE,de;q=0.9,en-US;q=0.7,en;q=0.6

JOBAGENT_FETCH_HTTP_TIMEOUT=35
JOBAGENT_FETCH_HTTP_RETRIES=2
JOBAGENT_FETCH_HTTP_BACKOFF_BASE=3.0

JOBAGENT_FETCH_DELAY_MIN_SEC=6.0
JOBAGENT_FETCH_DELAY_MAX_SEC=12.0
JOBAGENT_FETCH_FAILURE_BACKOFF_SEC=5.0

JOBAGENT_FETCH_PW_WAIT_UNTIL=domcontentloaded
JOBAGENT_FETCH_PW_TIMEOUT_MS=45000

JOBAGENT_FETCH_ROBOTS_TTL_SEC=86400
# JOBAGENT_FETCH_ACCESS_DENIED_MARKERS=access denied,request blocked,captcha required,forbidden,...

# Cache
JOBAGENT_CACHE_ENABLED=true
JOBAGENT_CACHE_TTL_DAYS=7
JOBAGENT_CACHE_VERSION=v2
JOBAGENT_CACHE_PER_PROFILE=true

# Auth / JWT
JOBAGENT_JWT_SECRET=CHANGE_ME_TO_A_LONG_RANDOM_STRING
JOBAGENT_JWT_ALG=HS256
JOBAGENT_JWT_EXPIRES_MIN=120
